  },
  "main": "/src/js/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "versionup": "versiony --patch",
    "wpk-watch": "set NODE_ENV=development && webpack -d                       --config webpack.config.js --watch --mode development",
    "wpk-dev": "set NODE_ENV=development && webpack -d                       --config webpack.config.js --mode development",
    "wpk-prod": "set NODE_ENV=production  && webpack -p                       --config webpack.config.js --mode production",
    "wpk-serv": "set NODE_ENV=development && webpack-dev-server --progress --config webpack.config.js --open --colors",
    "wpk-serv-speed": "set NODE_ENV=development && webpack-dev-server -d --progress --config webpack.config.js --open --colors",
    "wpk-hot": "set NODE_ENV=development && webpack-dev-server -d --progress --config webpack.config.js --open --colors --inline --hot"
  },
  "dependencies": {
